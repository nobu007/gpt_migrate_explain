# UCG DevOps Environment Variables
# Copy this file to .env and set your actual values

# GitHub Settings
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITHUB_ORGANIZATION=nobu007

# AI API Keys
OPENAI_API_KEY=sk-your-openai-api-key-here
CLAUDE_API_KEY=your-claude-api-key-here
GLM_API_KEY=your-glm-api-key-here

# UCG System Settings
UCG_MASTER_SECRET_KEY=your-master-encryption-key-here

# Notification Settings (Optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your-webhook-url

# Package Publishing (Optional)
PYPI_API_TOKEN=pypi-your-token-here
DOCKER_HUB_TOKEN=your-docker-hub-token

# Environment
UCG_ENVIRONMENT=development

# Gemini Code Assist
GOOGLE_CLOUD_PROJECT=your-google-cloud-project-id
GEMINI_API_KEY=your-gemini-api-key-here

# should not use .venv
UV_NO_SYNC=1

# Monitoring Dashboard Settings
MONITOR_HOST=127.0.0.1
MONITOR_PORT=8000
MONITOR_DEBUG=false
MONITOR_AUTO_BROWSER=true
MONITOR_UPDATE_INTERVAL=30
MONITOR_MAX_CONNECTIONS=50
MONITOR_LOG_LEVEL=INFO

# Monitoring Security
MONITOR_API_KEY=your-monitoring-api-key-here
MONITOR_CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
MONITOR_RATE_LIMIT_ENABLED=false

# Monitoring SSL (Production)
MONITOR_SSL_ENABLED=false
MONITOR_SSL_CERT_FILE=/path/to/ssl/cert.pem
MONITOR_SSL_KEY_FILE=/path/to/ssl/key.pem

# Monitoring Storage
MONITOR_LOG_FILE=/var/log/ucg-monitoring/monitoring.log
MONITOR_BACKUP_DIR=/var/backups/ucg-monitoring

# Monitoring Integrations
MONITOR_SLACK_WEBHOOK=https://hooks.slack.com/services/your-webhook-url
MONITOR_EMAIL_SMTP_SERVER=smtp.gmail.com
MONITOR_EMAIL_USERNAME=your-email@domain.com
MONITOR_EMAIL_PASSWORD=your-email-password
